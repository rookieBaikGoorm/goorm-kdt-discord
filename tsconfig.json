{
  "$schema": "https://json.schemastore.org/tsconfig",
	"display": "DiscordBot",
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2022",
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "strict": true,
    "skipLibCheck": true,
    "strictBindCallApply": false,
    "baseUrl": "./",
    "paths": {
      "#/*": ["src/*"],
    },
  }
}
